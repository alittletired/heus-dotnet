{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "dist",
    "target": "ESNext",
    "isolatedModules": false,
    "noEmit": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "strictNullChecks": false,
    "typeRoots": ["./node_modules/@types", "./typings", "../typings"]
  },
  "include": ["src/server/**/*.ts", "next-env.d.ts", "typings"]
}
