{
  "name": "mall-admin-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev ",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "api": "ts-node --project ./tsconfig.node.json scripts/api",
    "menu": "ts-node --project ./tsconfig.node.json ./scripts/menu.ts",
    "routes": "ts-node --project ./tsconfig.node.json ./scripts/routes.ts",
    "tsc": "tsc --noEmit --incremental"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@emotion/css": "^11.10.6",
    "@types/prettier": "^2.7.2",
    "antd": "5.3.2",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "jotai": "^1.13.1",
    "next": "^13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.3.2",
    "ts-node": "^10.9.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.2/xlsx-0.19.2.tgz"
  },
  "devDependencies": {
    "@types/node": "^18.15.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.6",
    "typescript": "5.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "prettier --write"
    ],
    "*.{js,json,css,md}": "prettier --write"
  }
}
