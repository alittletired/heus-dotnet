{
  "name": "enroll-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev ",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "api": "ts-node --project ./tsconfig.node.json scripts/api"
