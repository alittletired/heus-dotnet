{
  "name": "enroll-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev ",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "api": "ts-node --project ./tsconfig.node.json scripts/api",
    "menu": "ts-node --project ./tsconfig.node.json ./scripts/menu.ts",
    "routes": "ts-node --project ./tsconfig.node.json ./scripts/routes.ts",
    "tsc": "tsc --noEmit --incremental"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@types/prettier": "^2.7.2",
    "antd": "5.1.6",
    "axios": "^1.2.1",
    "dayjs": "^1.11.7",
    "exceljs": "^4.3.0",
    "jotai": "^1.13.1",
    "next": "^13.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "typescript": "4.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "prettier --write"
    ],
    "*.{js,json,css,md}": "prettier --write"
  }
}
