  },
  "dependencies": {
    "@swc/core": "^1.3.14",
    "@swc/helpers": "^0.4.12",
    "@types/prettier": "^2.7.1",
    "@types/react-is": "^17.0.3",
    "antd": "5.0.0-alpha.34",
    "axios": "^1.1.3",
    "exceljs": "^4.3.0",
    "next": "^13.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "recoil": "^0.7.6",
    "regenerator-runtime": "^0.13.10",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@types/node": "18.11.2",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.6",
    "eslint": "8.25.0",
    "eslint-config-next": "^13.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "4.8.4"
  },
  "husky": {
    "hooks": {
